{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@apollo/client": "^3.3.19",
    "antd": "^4.15.1",
    "axios": "^0.21.1",
    "dotenv-cli": "^4.0.0",
    "graphql": "^15.5.0",
    "history": "^4.10.1",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.0.0",
    "node-sass": "^5.0.0",
    "prettier": "2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0"
  },
  "scripts": {
    "start": "PORT=process.env.PORT react-scripts start",
    "build": "dotenv -e .env.development react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen",
    "tsc": "tsc --noEmit",
    "lint-scss": "stylelint '**/*.scss'",
    "lint-all": "tsc --noEmit && eslint ./src --ext .js,.jsx && stylelint '**/*.scss'",
    "lint-fix-all": "tsc --noEmit && eslint ./src --ext .js --fix && stylelint '**/*.scss' --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
